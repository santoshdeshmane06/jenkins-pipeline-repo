pipeline
{
agent any
stages
      { stage ('stage-1')
       {    steps
        {      sh 'echo downloading code'

        } 
       }
	stage('stage-2')
        { steps 
           { sh 'echo compiling code'
           }
        }


stage ('stage-3')

        { steps
          { sh 'echo building'
          }
        }


     stage ('stage-4')

        { steps
          { sh 'echo deploy'
          }
        }
      }
}
