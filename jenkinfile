pipeline{
  agent any
  stages
  {
      stage ('git clone')
      {
          steps
          { sh 'echo downloading code'}
      }
      stage ('code compile')
      {
          steps
          {
              sh 'echo code is compiling'
          }
      }
      stage ('code execution')
      {
          steps
          {
              sh 'echo code is executing'
          }
      }
      stage (' code Build')
      {
          steps{
              sh 'echo Build'
          }
      }
  }
}
